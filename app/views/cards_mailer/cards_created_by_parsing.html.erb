<h2><%= t('.heading') %></h2>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th><%= t('dashboard.cards.index.original_text') %></th>
      <th><%= t('dashboard.cards.index.translated_text') %></th>
    </tr>
  </thead>
  <tbody>
  <% @cards.each do |card| %>
      <tr>
        <td><%= card.original_text %></td>
        <td><%= card.translated_text %></td>
      </tr>
  <% end %>
  </tbody>
</table>
